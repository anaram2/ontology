        <rdfs:range rdf:resource="http://example.com/BallpointPenOntology/Pen"/>
    </owl:ObjectProperty>
    


    <!-- http://example.com/BallpointPenOntology/isFeatureOf -->

    <owl:ObjectProperty rdf:about="http://example.com/BallpointPenOntology/isFeatureOf"/>
    


